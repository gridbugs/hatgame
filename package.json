{
  "name": "another-webapp-starting-point",
  "version": "1.0.0",
  "description": "Boilerplate for webapp with client and server written in typescript",
  "scripts": {
    "build:server": "webpack --config webpack.config.server.js --mode production",
    "build:client": "webpack --config webpack.config.client.js --mode production",
    "watch:client": "webpack --config webpack.config.client.js --mode production --watch",
    "build": "node run build:client && node run build:client",
    "start": "node ./dist/app.js"
  },
  "author": "Stephen Sherratt",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
